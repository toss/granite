{
  "name": "@granite-js/pulumi-aws",
  "type": "module",
  "version": "0.1.23-next.7",
  "main": "./dist/index.cjs",
  "types": "./dist/index.d.ts",
  "module": "./dist/index.js",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    },
    "./lambda/origin-request": {
      "import": {
        "types": "./dist/lambda/origin-request.d.ts",
        "default": "./dist/lambda/origin-request.js"
      },
      "require": {
        "types": "./dist/lambda/origin-request.d.cts",
        "default": "./dist/lambda/origin-request.cjs"
      }
    },
    "./lambda/origin-response": {
      "import": {
        "types": "./dist/lambda/origin-response.d.ts",
        "default": "./dist/lambda/origin-response.js"
      },
      "require": {
        "types": "./dist/lambda/origin-response.d.cts",
        "default": "./dist/lambda/origin-response.cjs"
      }
    },
    "./lambda/auto-cache-removal": {
      "import": {
        "types": "./dist/lambda/auto-cache-removal.d.ts",
        "default": "./dist/lambda/auto-cache-removal.js"
      },
      "require": {
        "types": "./dist/lambda/auto-cache-removal.d.cts",
        "default": "./dist/lambda/auto-cache-removal.cjs"
      }
    }
  },
  "files": [
    "dist",
    "lambda",
    "package.json"
  ],
  "scripts": {
    "prepack": "yarn build",
    "typecheck": "tsc --noEmit",
    "test": "vitest --run --coverage",
    "build": "rm -rf dist && tsup"
  },
  "devDependencies": {
    "@aws-sdk/client-cloudfront": "^3.812.0",
    "@granite-js/deployment-manager": "workspace:*",
    "@granite-js/utils": "workspace:*",
    "@pulumi/aws": "^6.80.0",
    "@pulumi/pulumi": "^3.170.0",
    "@types/aws-lambda": "^8.10.149",
    "@types/node": "^22.10.2",
    "@vitest/coverage-v8": "^3.1.4",
    "aws-lambda": "^1.0.7",
    "aws-sdk-client-mock": "^4.1.0",
    "es-toolkit": "^1.39.8",
    "tsup": "^8.5.0",
    "typescript": "^5.8.3",
    "uuid": "^11.1.0",
    "vitest": "^3.1.4"
  },
  "peerDependencies": {
    "@pulumi/aws": "*",
    "@pulumi/pulumi": "*"
  },
  "dependencies": {
    "oxc-transform": "^0.71.0"
  }
}
