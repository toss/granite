{
  "name": "@granite-js/plugin-sentry",
  "type": "module",
  "version": "1.0.4",
  "description": "Plugin for integrating Sentry with Granite applications",
  "scripts": {
    "prepack": "yarn build",
    "typecheck": "tsc --noEmit",
    "test": "vitest --run",
    "build": "tsdown"
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/toss/granite.git",
    "directory": "packages/plugin-sentry"
  },
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    },
    "./package.json": "./package.json"
  },
  "files": [
    "dist"
  ],
  "devDependencies": {
    "@vitest/coverage-v8": "^4.0.12",
    "tsdown": "catalog:tools",
    "typescript": "catalog:tools",
    "vitest": "^4.0.12"
  },
  "dependencies": {
    "@granite-js/plugin-core": "workspace:*",
    "@granite-js/utils": "workspace:*",
    "@sentry/cli": "^2.45.0",
    "es-toolkit": "^1.39.8",
    "execa": "^5"
  },
  "sideEffects": false
}
