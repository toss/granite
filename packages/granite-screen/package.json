{
  "name": "@granite-js/screen",
  "type": "module",
  "version": "0.1.34",
  "description": "Brownfield Screen for React Native",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.js"
    },
    "./package.json": "./package.json"
  },
  "files": [
    "dist",
    "src",
    "ios",
    "android",
    "gradle-plugin/src",
    "gradle-plugin/gradle",
    "gradle-plugin/build/libs",
    "gradle-plugin/build.gradle.kts",
    "gradle-plugin/settings.gradle.kts",
    "gradle-plugin/gradle.properties",
    "gradle-plugin/gradlew",
    "gradle-plugin/gradlew.bat",
    "gradle-plugin/README.md",
    "gradle-plugin/LICENSE",
    "react-native.config.js",
    "!ios/build",
    "!android/build",
    "!android/gradle",
    "!android/gradlew",
    "!android/gradlew.bat",
    "!android/local.properties",
    "!**/__tests__",
    "!**/__fixtures__",
    "!**/__mocks__",
    "!**/.*",
    "GraniteScreen.podspec",
    "package.json"
  ],
  "scripts": {
    "build:gradle-plugin": "cd gradle-plugin && ./gradlew jar",
    "test:gradle-plugin": "cd gradle-plugin && ./gradlew test",
    "prepack": "cd gradle-plugin && ./gradlew jar"
  },
  "nx": {
    "targets": {
      "build:gradle-plugin": {
        "cache": true,
        "inputs": [
          "{projectRoot}/gradle-plugin/src/**",
          "{projectRoot}/gradle-plugin/build.gradle.kts",
          "{projectRoot}/gradle-plugin/settings.gradle.kts",
          "{projectRoot}/gradle-plugin/gradle.properties",
          "{projectRoot}/gradle-plugin/gradle/libs.versions.toml"
        ],
        "outputs": ["{projectRoot}/gradle-plugin/build/libs"]
      },
      "test:gradle-plugin": {
        "cache": true,
        "inputs": [
          "{projectRoot}/gradle-plugin/src/**",
          "{projectRoot}/gradle-plugin/build.gradle.kts"
        ],
        "dependsOn": ["build:gradle-plugin"]
      }
    }
  },
  "license": "Apache-2.0",
  "stableVersion": "1.0.0-next.97"
}
