{
  "name": "@granite-js/mpack",
  "version": "0.1.34-next.35",
  "description": "A bundler for Granite apps",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/toss/granite.git",
    "directory": "packages/mpack"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./experimental": {
      "types": "./dist/experimental/index.d.ts",
      "default": "./dist/experimental/index.js"
    },
    "./plugins": {
      "types": "./dist/plugins/index.d.ts",
      "default": "./dist/plugins/index.js"
    },
    "./vendors": {
      "types": "./dist/vendors/index.d.ts",
      "default": "./dist/vendors/index.js"
    },
    "./package.json": "./package.json"
  },
  "files": [
    "dist",
    "*.d.ts",
    "!**/testing",
    "!**/__tests__"
  ],
  "scripts": {
    "prepack": "yarn build",
    "typecheck": "tsc --noEmit",
    "build": "tsup && tsc -p tsconfig.build.json && node .scripts/copy-vendors-dts.mjs",
    "lint": "eslint .",
    "find-deadcode": "ts-prune",
    "test:no-parallel": "vitest --no-watch --no-file-parallelism --coverage"
  },
