{
  "name": "@granite-js/plugin-router",
  "type": "module",
  "version": "1.0.0",
  "description": "A Route Generator for Granite project",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/toss/granite.git",
    "directory": "packages/plugin-router"
  },
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "scripts": {
    "prepack": "yarn build",
    "typecheck": "tsc --noEmit",
    "lint": "eslint .",
    "test": "vitest --no-watch --coverage",
    "build": "tsdown"
  },
  "files": [
    "dist"
  ],
  "devDependencies": {
    "@types/node": "catalog:tools",
    "@vitest/coverage-v8": "^4.0.12",
    "tsdown": "catalog:tools",
    "typescript": "catalog:tools",
    "vitest": "^4.0.12"
  },
  "dependencies": {
    "@granite-js/plugin-core": "workspace:*",
    "@swc/core": "catalog:swc",
    "chokidar": "4.0.1",
    "es-toolkit": "^1.39.8"
  },
  "sideEffects": false
}
