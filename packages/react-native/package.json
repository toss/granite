{
  "name": "@granite-js/react-native",
  "version": "0.1.23-next.6",
  "description": "The Granite Framework",
  "bin": {
    "granite": "./bin/cli.js"
  },
  "scripts": {
    "typecheck": "tsc --noEmit",
    "test": "vitest --no-watch --coverage",
    "lint": "eslint .",
    "build": "tsup && tsc -p tsconfig.build.json",
    "prepack": "yarn build"
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./src/index.ts"
    },
    "./async-bridges": {
      "import": {
        "types": "./dist/async-bridges.d.mts",
        "default": "./dist/async-bridges.mjs"
      },
      "require": {
        "types": "./dist/async-bridges.d.ts",
        "default": "./dist/async-bridges.js"
      }
    },
    "./constant-bridges": {
      "import": {
        "types": "./dist/constant-bridges.d.mts",
        "default": "./dist/constant-bridges.mjs"
      },
      "require": {
        "types": "./dist/constant-bridges.d.ts",
        "default": "./dist/constant-bridges.js"
      }
    },
    "./config": {
      "import": {
        "types": "./dist/config.d.mts",
        "default": "./dist/config.mjs"
      },
      "require": {
        "types": "./dist/config.d.ts",
        "default": "./dist/config.js"
      }
    },
    "./cli": {
      "import": {
        "types": "./dist/cli.d.mts",
        "default": "./dist/cli.mjs"
      },
      "require": {
        "types": "./dist/cli.d.ts",
        "default": "./dist/cli.js"
      }
    },
    "./jest": {
      "import": {
        "types": "./dist/jest.d.mts",
        "default": "./dist/jest.mjs"
      },
      "require": {
        "types": "./dist/jest.d.ts",
        "default": "./dist/jest.js"
      }
    },
    "./types": {
      "types": "./dist/types/global.d.ts"
    },
    "./package.json": "./package.json"
  },
  "files": [
    "src",
    "dist",
    "bin",
    "config.js",
    "cli.js",
    "*.d.ts"
  ],
  "devDependencies": {
    "@babel/core": "^7.24.9",
    "@babel/preset-env": "^7.24.8",
    "@babel/preset-typescript": "^7.24.7",
    "@granite-js/native": "workspace:*",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/react": "^16.1.0",
    "@types/babel__core": "^7",
    "@types/babel__preset-env": "^7",
    "@types/node": "^22.10.2",
    "@types/react": "catalog:react-native",
    "@vitest/coverage-v8": "^2.1.8",
    "brick-module": "^0.1.12",
    "esbuild": "0.25.9",
    "eslint": "^9.7.0",
    "jsdom": "^25.0.1",
    "react": "catalog:react-native",
    "react-native": "catalog:react-native",
    "tsup": "^8.5.0",
    "typescript": "5.8.3",
    "vitest": "^2.1.8"
  },
  "peerDependencies": {
    "@granite-js/native": "0.1.23-next.6",
    "@types/react": "*",
    "brick-module": "*",
    "react": "*",
    "react-native": "*"
  },
  "dependencies": {
    "@granite-js/cli": "workspace:*",
    "@granite-js/image": "workspace:*",
    "@granite-js/jest": "workspace:*",
    "@granite-js/lottie": "workspace:*",
    "@granite-js/mpack": "workspace:*",
    "@granite-js/plugin-core": "workspace:*",
    "@granite-js/style-utils": "workspace:*",
    "es-toolkit": "^1.39.8",
    "react-native-url-polyfill": "1.3.0"
  }
}
