{
  "name": "@granite-js/react-native",
  "version": "0.1.34-next.43",
  "description": "The Granite Framework",
  "type": "module",
  "bin": {
    "granite": "./bin/cli.js"
  },
  "scripts": {
    "typecheck": "tsc --noEmit",
    "test": "vitest --no-watch --coverage",
    "lint": "eslint .",
    "build": "bob build && tsc --project tsconfig.build.json",
    "prepack": "yarn build"
  },
  "main": "./dist/module/index.js",
  "types": "./dist/typescript/index.d.ts",
  "react-native": "./src/index.ts",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/toss/granite.git",
    "directory": "packages/react-native"
  },
  "exports": {
    ".": {
      "types": "./dist/typescript/index.d.ts",
      "react-native": "./src/index.ts",
      "browser": "./src/index.ts",
      "default": "./dist/module/index.js"
    },
    "./async-bridges": {
      "types": "./dist/typescript/async-bridges.d.ts",
      "react-native": "./src/async-bridges.ts",
      "browser": "./src/async-bridges.ts",
      "default": "./dist/module/async-bridges.js"
    },
    "./constant-bridges": {
      "types": "./dist/typescript/constant-bridges.d.ts",
      "react-native": "./src/constant-bridges.ts",
      "browser": "./src/constant-bridges.ts",
      "default": "./dist/module/constant-bridges.js"
    },
    "./config": {
      "types": "./dist/typescript/config.d.ts",
      "react-native": "./src/config.ts",
      "browser": "./src/config.ts",
      "default": "./dist/module/config.js"
    },
    "./cli": {
      "types": "./dist/typescript/cli.d.ts",
      "react-native": "./src/cli.ts",
      "browser": "./src/cli.ts",
      "default": "./dist/module/cli.js"
    },
    "./jest": {
      "types": "./dist/typescript/jest.d.ts",
      "react-native": "./src/jest.ts",
      "browser": "./src/jest.ts",
      "default": "./dist/module/jest.js"
    },
    "./types": {
      "types": "./dist/typescript/types/global.d.ts"
    },
    "./package.json": "./package.json"
  },
  "files": [
    "src",
    "dist",
    "bin",
    "*.d.ts"
  ],
  "devDependencies": {
    "@babel/core": "7.28.5",
    "@babel/preset-env": "7.28.5",
    "@babel/preset-typescript": "7.28.5",
    "@granite-js/native": "workspace:*",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/react": "^16.1.0",
    "@types/babel__core": "^7",
    "@types/babel__preset-env": "^7",
    "@types/node": "^22.10.2",
    "@types/react": "catalog:react-native",
    "@types/react-dom": "catalog:react-native",
    "@vitest/coverage-v8": "^4.0.12",
    "del-cli": "^6.0.0",
    "eslint": "^9.7.0",
    "jsdom": "^25.0.1",
    "react": "catalog:react-native",
    "react-native": "catalog:react-native",
    "react-native-builder-bob": "0.40.17",
    "typescript": "5.8.3",
    "vitest": "^4.0.12",
    "zod": "^4.1.12"
  },
  "peerDependencies": {
    "@granite-js/native": "workspace:*",
    "@types/react": "*",
    "react": "*",
    "react-native": "*"
  },
  "dependencies": {
    "@granite-js/blur-view": "workspace:*",
    "@granite-js/cli": "workspace:*",
    "@granite-js/image": "workspace:*",
    "@granite-js/jest": "workspace:*",
    "@granite-js/lottie": "workspace:*",
    "@granite-js/mpack": "workspace:*",
    "@granite-js/plugin-core": "workspace:*",
    "@granite-js/style-utils": "workspace:*",
    "@granite-js/video": "workspace:*",
    "@standard-schema/spec": "^1.0.0",
    "brick-module": "catalog:brick-module",
    "es-toolkit": "^1.39.8",
    "react-native-url-polyfill": "3.0.0"
  },
  "sideEffects": [
    "dist/module/async-bridges.*",
    "dist/module/constant-bridges.*"
  ],
  "react-native-builder-bob": {
    "source": "src",
    "output": "dist",
    "targets": [
      [
        "module",
        {
          "esm": true
        }
      ]
    ]
  }
}
