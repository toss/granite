{
  "name": "@granite-js/plugin-radon",
  "type": "module",
  "version": "0.1.0",
  "description": "Radon compilation plugin for Granite",
  "scripts": {
    "prepack": "yarn build",
    "typecheck": "tsc --noEmit",
    "test": "vitest --run --coverage --passWithNoTests",
    "build": "tsdown"
  },
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./lib/*": "./dist/lib/*",
    "./package.json": "./package.json"
  },
  "files": [
    "dist"
  ],
  "devDependencies": {
    "@babel/core": "7.23.9",
    "@babel/template": "^7.27.2",
    "@granite-js/plugin-core": "workspace:*",
    "@types/babel__core": "^7.20.5",
    "@types/babel__template": "^7",
    "@types/node": "^22.10.2",
    "tsdown": "^0.11.13",
    "typescript": "^5.8.3"
  },
  "peerDependencies": {
    "react": "*",
    "react-native": "*"
  },
  "sideEffects": false
}
