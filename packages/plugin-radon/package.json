{
  "name": "@granite-js/plugin-radon",
  "type": "module",
  "version": "0.1.0",
  "description": "Radon compilation plugin for Granite",
  "scripts": {
    "prepack": "yarn build",
    "typecheck": "tsc --noEmit",
    "test": "vitest --run --coverage --passWithNoTests",
    "build": "tsdown"
  },
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./babel": "./dist/babel.js",
    "./babel.js": "./dist/babel.js",
    "./lib/*": "./dist/lib/*",
    "./lib/react_devtools_polyfill": {
      "import": "./dist/lib/react_devtools_polyfill.js",
      "require": "./dist/lib/react_devtools_polyfill.cjs"
    },
    "./package.json": "./package.json",
    "./lib/": "./dist/lib/"
  },
  "files": [
    "dist",
    "src"
  ],
  "devDependencies": {
    "@babel/core": "7.23.9",
    "@babel/template": "^7.27.2",
    "@types/babel__core": "^7.20.5",
    "@types/babel__template": "^7",
    "@types/node": "^22.10.2",
    "@vitest/coverage-v8": "^3.1.3",
    "tsdown": "^0.11.13",
    "typescript": "^5.8.3",
    "vitest": "^3.1.3"
  },
  "dependencies": {
    "@granite-js/plugin-core": "workspace:*",
    "@granite-js/utils": "workspace:*",
    "es-toolkit": "^1.38.0",
    "execa": "^5",
    "source-map": "^0.8.0-beta.0"
  },
  "peerDependencies": {
    "@babel/types": "*",
    "react": "*",
    "react-native": "*"
  },
  "sideEffects": false
}
